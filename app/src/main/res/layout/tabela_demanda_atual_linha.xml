        android:textColor="@android:color/black"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/tv_nov"
        android:layout_width="50sp"
        android:layout_height="30sp"
        android:gravity="center"
        android:text="@string/txt_tv_zero"
