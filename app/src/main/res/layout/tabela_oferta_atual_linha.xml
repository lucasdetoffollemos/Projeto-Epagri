<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">


    <Spinner
        android:id="@+id/spinner_tipoPiquete"
        android:layout_width="100dp"
        android:layout_height="30sp"
        android:spinnerMode="dropdown"
        android:layout_weight="1"/>

    <Spinner
        android:id="@+id/spinner_condPiquete"
        android:layout_width="80dp"
        android:layout_height="30sp"
        android:spinnerMode="dropdown"
        android:layout_weight="1"/>

    <EditText
        android:id="@+id/et_area"
        android:layout_marginLeft="15sp"
        android:layout_width="60sp"
        android:layout_height="30sp"
        android:layout_weight="1"
        android:background="@null"
        android:gravity="center"
        android:inputType="textPersonName"
        android:hint="@string/hint_et_area"
        android:textSize="14sp"
        android:autofillHints=""
        android:layout_marginStart="15sp" />


    <TextView
        android:id="@+id/tv_prod"
        android:layout_height="20sp"
        android:gravity="center"
        android:layout_weight="1"
        android:text="@string/txt_tv_producaoEstimada"
        android:textColor="@android:color/black"
        android:textSize="14sp"
        android:layout_marginRight="25dp"
        android:layout_marginEnd="25dp"/>





    <TextView
        android:id="@+id/tv_jan"
        android:layout_width="70sp"
        android:layout_height="30sp"
        android:gravity="center"
        android:text="@string/txt_tv_zero"
        android:textColor="@android:color/black"
        android:textSize="14sp"
        android:paddingRight="8dp"
        android:paddingEnd="8dp"
        tools:ignore="RtlSymmetry" />


    <TextView
        android:id="@+id/tv_fev"
        android:layout_width="70sp"
        android:layout_height="30sp"
        android:gravity="center"
        android:text="@string/txt_tv_zero"
